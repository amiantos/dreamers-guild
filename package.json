{
  "name": "aislingeach-web",
  "version": "1.0.0",
  "description": "A web-based AI Horde image generation application",
  "main": "server/server.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon server/server.js",
    "client:dev": "cd vue_client && npm run dev",
    "server": "node server/server.js",
    "client:build": "cd vue_client && npm run build",
    "client:preview": "cd vue_client && npm run preview",
    "install:all": "npm install && cd vue_client && npm install"
  },
  "keywords": [
    "ai",
    "image-generation",
    "ai-horde",
    "express",
    "vue"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^11.7.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "sharp": "^0.33.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  }
}
